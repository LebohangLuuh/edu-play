<?xml version="1.0" encoding="utf-8"?>
    <androidx.drawerlayout.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:id="@+id/drawer_Layout"
        tools:context="Afrikaans.Afrikaans_Version"
        tools:openDrawer="start">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/green">

            <FrameLayout
                android:layout_width="match_parent"
                android:id="@+id/frameLayout"
                android:layout_height="match_parent"/>

            <ImageButton
                android:id="@+id/days_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/days_foreground"
                app:flow_verticalGap="2dp"
                app:layout_constraintEnd_toEndOf="@+id/editTextText_Afr"
                app:layout_constraintHorizontal_bias="0.526"
                app:layout_constraintStart_toEndOf="@+id/count_Afr"
                app:layout_constraintTop_toTopOf="@+id/count_Afr"
                tools:ignore="DuplicateSpeakableTextCheck,SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/count_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/number_foreground"
                app:layout_constraintEnd_toEndOf="@+id/editTextText_Afr"
                app:layout_constraintHorizontal_bias="0.501"
                app:layout_constraintStart_toStartOf="@+id/editTextText_Afr"
                app:layout_constraintTop_toBottomOf="@+id/editTextText_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/vowels_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/vowels_foreground"
                app:flow_verticalGap="2dp"
                app:layout_constraintEnd_toStartOf="@+id/count_Afr"
                app:layout_constraintStart_toStartOf="@+id/editTextText_Afr"
                app:layout_constraintTop_toTopOf="@+id/count_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/money_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/money_foreground"
                app:layout_constraintEnd_toStartOf="@+id/days_Afr"
                app:layout_constraintStart_toEndOf="@+id/vowels_Afr"
                app:layout_constraintTop_toBottomOf="@+id/count_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/calendar_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/calender_foreground"
                app:layout_constraintEnd_toEndOf="@+id/money_Afr"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/province_Afr"
                app:layout_constraintTop_toBottomOf="@+id/money_Afr"
                tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/editTextText_Afr"
                android:layout_width="369dp"
                android:layout_height="55dp"
                android:ems="10"
                android:importantForAutofill="no"
                android:inputType="none"
                android:text="@string/choose_an_activity"
                android:textAlignment="center"
                android:textColor="#1565C0"
                android:textSize="30sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@id/toolbar_Afr"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.502"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.04"
                tools:ignore="LabelFor,TextSizeCheck" />

            <ImageButton
                android:id="@+id/careers_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/career_foreground"
                app:layout_constraintEnd_toEndOf="@+id/calendar_Afr"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/seasons_Afr"
                app:layout_constraintTop_toBottomOf="@+id/calendar_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/alphabets_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/alphabets_foreground"
                app:layout_constraintEnd_toEndOf="@+id/shapes_Afr"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/careers_Afr"
                app:layout_constraintTop_toBottomOf="@+id/colors_Afr"
                tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/colors_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/colors_foreground"
                app:layout_constraintEnd_toEndOf="@+id/shapes_Afr"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/calendar_Afr"
                app:layout_constraintTop_toBottomOf="@+id/shapes_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/shapes_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/shapes_foreground"
                app:layout_constraintEnd_toEndOf="@+id/days_Afr"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/money_Afr"
                app:layout_constraintTop_toBottomOf="@+id/days_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/province_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/provinces_foreground"
                app:layout_constraintCircleRadius="0dp"
                app:layout_constraintEnd_toStartOf="@+id/money_Afr"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/vowels_Afr"
                app:layout_constraintTop_toBottomOf="@+id/vowels_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/seasons_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/seasons_foreground"
                app:layout_constraintEnd_toStartOf="@+id/calendar_Afr"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/province_Afr"
                app:layout_constraintTop_toBottomOf="@+id/province_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/phonics_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/phonics_foreground"
                app:layout_constraintEnd_toStartOf="@+id/careers_Afr"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/seasons_Afr"
                app:layout_constraintTop_toBottomOf="@+id/seasons_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/animals_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/animals_foreground"
                app:layout_constraintCircleRadius="0dp"
                app:layout_constraintEnd_toEndOf="@+id/phonics_Afr"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/phonics_Afr"
                app:layout_constraintTop_toBottomOf="@+id/phonics_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/planets_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/planets_foreground"
                app:layout_constraintEnd_toEndOf="@+id/careers_Afr"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/careers_Afr"
                app:layout_constraintTop_toBottomOf="@+id/careers_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageButton
                android:id="@+id/traffic_Light_Afr"
                android:layout_width="110dp"
                android:layout_height="149dp"
                android:backgroundTint="@color/green"
                android:foreground="@mipmap/robot_foreground"
                app:layout_constraintEnd_toEndOf="@+id/alphabets_Afr"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/alphabets_Afr"
                app:layout_constraintTop_toBottomOf="@+id/alphabets_Afr"
                tools:ignore="SpeakableTextPresentCheck"
                tools:srcCompat="@tools:sample/avatars" />

            <androidx.appcompat.widget.Toolbar
                android:layout_width="match_parent"
                app:title="Kies 'n aktiwiteit"
                app:titleTextColor="@color/blue_Font"
                android:textAlignment="center"
                android:id="@+id/toolbar_Afr"
                android:background="@color/green"
                android:elevation="3dp"
                android:layout_height="wrap_content"
                tools:ignore="MissingConstraints" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <com.google.android.material.navigation.NavigationView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:id="@+id/nav_view_Afr"
            app:headerLayout="@layout/header"
            app:menu="@menu/menu_bar" />

    </androidx.drawerlayout.widget.DrawerLayout>
